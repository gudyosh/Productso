{
    "name": "gudyosh/Productso",
    "type": "library",
    "description": ":package",
    "keywords": [
        "gudyosh",
        "productso"
    ],
    "homepage": "https://github.com/gudyosh/productso",
    "license": "MIT",
    "authors": [
        {
            "name": ":gudyosh",
            "email": ":gudyosh87@gmail.com",
            "homepage": ":www.unm.uz",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "gudyosh\\Productso\\": "src/",
            "gudyosh\\Productso\\Http\\Controllers\\": "src/Http/Controllers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gudyosh\\productso\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
